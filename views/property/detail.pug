block tab
    // .section-block
    .section-block
        // .metric-row
        .metric-row.metric-flush
            // metric column
            .col
                // .metric
                a.metric.metric-bordered.align-items-center(href='page-team-members.html')
                    h2.metric-label  Members 
                    p.metric-value.h3
                        sub
                            i.oi.oi-people
                        span.value 12
                // /.metric
            // /metric column
            // metric column
            .col
                // .metric
                a.metric.metric-bordered.align-items-center(href='page-team-projects.html')
                    h2.metric-label  Projects 
                    p.metric-value.h3
