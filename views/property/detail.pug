                            i.oi.oi-fork
                        span.value 26
                // /.metric
            // /metric column
            // metric column
            .col
                // .metric
                a.metric.metric-bordered.align-items-center(href='page-team-projects.html')
                    h2.metric-label  Active Projects 
                    p.metric-value.h3
                        sub
                            i.oi.oi-timer.fa-lg
                        span.value 5
                // /.metric
            // /metric column
        // /.metric-row
    // /.section-block
    // .section-block
    .section-block.d-flex.justify-content-between.align-items-center.my-3
        h1.section-title.mb-0  Achievement 
        // .dropdown
        .dropdown
            button.btn.btn-secondary(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                span This Month
                i.fa.fa-fw.fa-caret-down
            .dropdown-arrow.dropdown-arrow-right
            // .dropdown-menu
            .dropdown-menu.dropdown-menu-right.dropdown-menu-md.stop-propagation
                // .custom-control
                .custom-control.custom-radio
                    input#dpToday.custom-control-input(type='radio' name='dpFilter' value='0')
                    label.custom-control-label.d-flex.justify-content-between(for='dpToday')
                        span Today
                        span.text-muted Mar 27
                // /.custom-control
                // .custom-control
                .custom-control.custom-radio
                    input#dpYesterday.custom-control-input(type='radio' name='dpFilter' value='1')
                    label.custom-control-label.d-flex.justify-content-between(for='dpYesterday')
                        span Yesterday
                        span.text-muted Mar 26
                // /.custom-control
                // .custom-control
                .custom-control.custom-radio
                    input#dpWeek.custom-control-input(type='radio' name='dpFilter' value='2')
                    label.custom-control-label.d-flex.justify-content-between(for='dpWeek')
                        span This Week
                        span.text-muted Mar 21-27
                // /.custom-control
                // .custom-control
                .custom-control.custom-radio
                    input#dpMonth.custom-control-input(type='radio' name='dpFilter' value='3' checked='')
                    label.custom-control-label.d-flex.justify-content-between(for='dpMonth')
                        span This Month
                        span.text-muted Mar 1-31
                // /.custom-control
                // .custom-control
                .custom-control.custom-radio
                    input#dpYear.custom-control-input(type='radio' name='dpFilter' value='4')
                    label.custom-control-label.d-flex.justify-content-between(for='dpYear')
                        span This Year
                        span.text-muted 2018
                // /.custom-control
                // .custom-control
                .custom-control.custom-radio
                    input#dpCustom.custom-control-input(type='radio' name='dpFilter' value='5')
                    label.custom-control-label(for='dpCustom') Custom
                    .custom-control-hint.my-1(for='dpCustom')
                        // datepicker:range
                        input.form-control(type='text' data-toggle='flatpickr' data-mode='range' data-date-format='Y-m-d')
                        // /datepicker:range
                // /.custom-control
            // /.dropdown-menu
        // /.dropdown
    // /.section-block
    // .card
    .card.card-body
